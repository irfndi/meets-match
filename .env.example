# MeetsMatch Environment Variables
# Copy this file to .env and fill in your values

# Required: Telegram Bot Token (get from @BotFather)
BOT_TOKEN=your_telegram_bot_token_here

# Required: PostgreSQL password (use a strong password in production)
POSTGRES_PASSWORD=your_secure_password_here

# Optional: PostgreSQL configuration (defaults shown)
# POSTGRES_USER=postgres
# POSTGRES_DB=meetsmatch
