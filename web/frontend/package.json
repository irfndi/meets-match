{
  "name": "meetsmatch-frontend",
  "version": "1.0.0",
  "description": "MeetsMatch React Frontend - Dating app with Telegram integration",
  "type": "module",
  "scripts": {
    "dev": "bun run vite",
    "build": "bun run tsc && bun run vite build",
    "preview": "bun run vite preview",
    "lint": "bun run oxlint src",
    "lint:fix": "bun run oxlint src --fix",
    "type-check": "bun run tsc --noEmit",
    "test": "bun run vitest",
    "test:ui": "bun run vitest --ui",
    "test:coverage": "bun run vitest --coverage"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "@tanstack/react-query": "^5.85.3",
    "axios": "^1.11.0",
    "zustand": "^5.0.7",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "lucide-react": "^0.539.0",
    "sonner": "^2.0.7",
    "@hookform/resolvers": "^5.2.1",
    "react-hook-form": "^7.62.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "vite": "^7.1.2",
    "typescript": "^5.9.2",
    "oxlint": "^1.11.2",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/user-event": "^14.6.1",
    "tailwindcss": "^4.1.12",
    "postcss": "^8.5.6",
    "autoprefixer": "^10.4.21"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.42",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "dating-app",
    "telegram",
    "frontend"
  ],
  "author": "MeetsMatch Team",
  "license": "MIT"
}